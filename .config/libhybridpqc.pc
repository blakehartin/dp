HYBRIDPQC_INCLUDE_DIR=C:/github/hybrid-pqc/build/include
HYBRIDPQC_LIB_DIR=C:/github/hybrid-pqc/build/lib

Name: hybridpqc
Description: C library for quantum resistant cryptography
Version: 0.0.1-dev
Cflags: -I${HYBRIDPQC_INCLUDE_DIR}
Ldflags: '-extldflags "-Wl,-stack_size -Wl,0x1000000"'
Libs: -L${HYBRIDPQC_LIB_DIR} -lhybridpqc

